{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "vite.config.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_next/.*",
      "dest": "/_next"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
