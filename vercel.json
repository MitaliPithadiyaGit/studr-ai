{
  "version": 2,
  "builds": [
    {
      "src": "remix.config.mjs",
      "use": "@remix-run/vercel"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
