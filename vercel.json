{
  "version": 2,
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/build/*", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.js" 
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    }
  ]
}